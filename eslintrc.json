{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-refresh/recommended"
  ],
  "plugins": [
    "react",
    "react-refresh"
  ],
  "rules": {
    "react-refresh/only-export-components": ["warn", { "allowConstantExport": true }],
    "no-unused-vars": ["error", { "varsIgnorePattern": "^publicId$" }]
  }
}